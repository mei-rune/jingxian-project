        <viewType id="jingxian.ui.views.outlookPage"/>
        <viewType id="jingxian.ui.views.messageExplorer"/>
      </includedViewTypes>
      <includedToolbars>
        <toolbar id="jingxian.ui.menu.mainMenu"/>
      </includedToolbars>
    </perspective>
  </extension>

